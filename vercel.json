{
    "installCommand": "yarn install --ignore-engines",
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node",
        "config": {
          "nodeVersion": "20.x"
        }
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }